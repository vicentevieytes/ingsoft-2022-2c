!methodRemoval: ReceptiveAccount #accountSummary stamp: 'LM 10/22/2022 18:02:58'!
accountSummary
	|res|
	
	res := OrderedCollection new .
	transactions do: [:aTransaction| 
			(aTransaction isKindOf: Deposit) ifTrue: [(res add: 'Depsito por ' , aTransaction value asString ,'.')].
			(aTransaction isKindOf: Withdraw) ifTrue: [(res add: 'Extraccin por ' , aTransaction value asString ,'.')].
			(aTransaction isKindOf: TransferDeposit ) ifTrue: [(res add: 'Depsito por transferencia de ' , aTransaction value asString ,'.')].
			(aTransaction isKindOf: TransferWithdraw ) ifTrue: [(res add: 'Extraccin por transferencia de ' , aTransaction value asString ,'.')			]
			].		
			
	res add:		('Balance = ' , self balance asString).
	^res	!

