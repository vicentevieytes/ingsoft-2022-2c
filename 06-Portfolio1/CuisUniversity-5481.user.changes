	^ 	(accounts anySatisfy: [:account| (account isInSubtree: anAccount) or: [anAccount allAccounts includes: account]]) 
		or: 
		(parents anySatisfy: [:aParent| aParent isAChildOfAncestor: anAccount])
		or:
		(anAccount isInSubtree: self)  or:
		(self = anAccount)
		
